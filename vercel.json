{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/create-checkout-session",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/upload",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}